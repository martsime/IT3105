{
  "data_source": "autoencoder_dense",
  "error_function": "cross_entropy",
  "optimizer": "adam",
  "learning_rate": 0.02,
  "delta_learning_rate": 0.999,
  "case_fraction": 1.0,
  "validation_fraction": 0.1,
  "test_fraction": 0.1,
  "validation_interval": 10,
  "minibatch_size": 32,
  "map_batch_size": 8,
  "steps": 1000,
  "testing": "equal",
  "hidden_layers": [
    {
      "size": 6,
      "weights": {
        "range": [-0.1, 0.1]
      },
      "visualize_layer": {
        "mappings": false,
        "dendrograms": true,
        "weights": {
          "start": false,
          "end": false
        },
        "biases": {
          "start": false,
          "end": false
        }
      }
    }
  ],
  "output_layer": {
    "activation": "sigmoid"
  },
  "visualization": {
    "dendrograms": false,
    "mappings": false,
    "error": true,
    "weights": {
      "start": false,
      "end": false
    },
    "biases": {
      "start": false,
      "end": false
    }
  }
}
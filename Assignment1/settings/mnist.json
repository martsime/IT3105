{
  "data_source": "mnist",
  "error_function": "mse",
  "optimizer": "adam",
  "learning_rate": 0.001,
  "delta_learning_rate": 0.9999,
  "case_fraction": 0.1,
  "validation_fraction": 0.1,
  "test_fraction": 0.1,
  "validation_interval": 20,
  "minibatch_size": 500,
  "map_batch_size": 10,
  "steps": 300,
  "hidden_layers": [
    {
      "size": 128,
      "activation": "relu"
    },
    {
      "size": 32,
      "activation": "relu"
    }
  ],
  "output_layer": {
    "activation": "softmax"
  },
  "visualization": {
    "dendrograms": false,
    "mappings": false,
    "weights": {
      "start": false,
      "end": false
    },
    "biases": {
      "start": false,
      "end": false
    },
    "error": true
  },
  "seed": false
}
